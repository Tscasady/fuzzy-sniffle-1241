<p>Chef: <%= @chef.name %></p>
<p>Dishes:</p>
<% @chef.dishes.each do |dish| %>
  <p><%= dish.name %></p>
<% end %>

<%= form_with model: @chef, local: true do |f| %>
  <%= f.label :dish_id %>
  <%= f.number_field :dish_id %>
  <%= f.submit "Add Dish" %>
<% end %>

<div id="popular_ingredients">
  <% @chef.popular_ingredients.each do |ingredient| %>
    <%= ingredient.name %>
  <% end %>
</div>

<%= link_to "All Ingredients", chef_ingredients_path(@chef) %>